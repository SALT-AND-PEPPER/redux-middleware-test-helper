{
  "extends": ["standard"],
  "parser": "babel-eslint",
  "env": {
    "mocha": true
  },
  "rules": {
    "no-return-assign": 0,
    "generator-star-spacing": 0,
    "object-curly-spacing": ["error", "always"],
    "arrow-parens": ["error", "as-needed"],
    "no-debugger": "error",
    "no-alert": "error",
    "comma-dangle": ["error", "always-multiline"]
  }
}
